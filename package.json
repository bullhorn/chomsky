{
  "name": "chomsky",
  "version": "0.1.3",
  "description": "A simple ES6 i18n engine.",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "npm run serve",
    "build": "gulp",
    "watch": "gulp watch",
    "clean": "gulp clean",
    "test": "npm run unit",
    "unit": "karma start --single-run",
    "tdd": "karma start",
    "serve": "concurrently \"npm run build\" \"http-server public\"",
    "compile": "babel src --i .spec.js --out-dir lib",
    "compile:watch": "babel src --ignore .spec.js --out-dir lib --watch",
    "prepublish": "npm run compile"
  },
  "main": "lib/chomsky.js",
  "author": "Kameron R. Sween <kameron@krsween.com>",
  "repository": "krsween/chomsky",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.6.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "babylon": "6.6.4",
    "browserify": "^13.0.0",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "eslint": "^2.6.0",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^5.0.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "^1.1.0",
    "karma-spec-reporter": "0.0.25",
    "object-assign": "^4.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack-stream": "^3.1.0",
    "intl": "^1.1.0",
    "parse-decimal-number": "^0.1.1",
    "numeral": "^1.5.3"

  },
  "dependencies": {
    "moment": "^2.12.0",
    "object-assign-deep": "0.0.4"
  }
}
